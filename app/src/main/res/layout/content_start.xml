<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"


    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"


    android:id="@+id/activity_start"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.lite.my.masterok.Start_Activity">


    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:onClick="onClickSt1"
        android:orientation="vertical">

        <Button
            android:id="@+id/button212"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/radius_armopojas"
            android:onClick="onClickFun8"
            android:text="Армопояс"
            android:textColor="@android:color/background_light"
            android:textSize="22sp" />

        <Button
            android:id="@+id/button2412"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/radius_plita"
            android:onClick="onClickFun8"
            android:text="Плита перекрытия"
            android:textColor="@android:color/background_light"
            android:textSize="22sp" />

        <Button
            android:id="@+id/button4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/radius_kirpish"
            android:onClick="onClickKirpish"
            android:text="Кирпич"
            android:textColor="@android:color/background_light"
            android:textSize="22sp" />

        <Button
            android:id="@+id/button9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/radius_bloki"
            android:onClick="onClickBloki"
            android:text="Блоки"
            android:textColor="@android:color/background_light"
            android:textSize="22sp" />

        <Button
            android:id="@+id/button21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/radius_plitka"
            android:onClick="onClickPlitka"
            android:text="Плитка"
            android:textColor="@android:color/background_light"
            android:textSize="22sp" />

        <Button
            android:id="@+id/button210"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/radius_stagka"
            android:onClick="onClickSt1"
            android:text="Стяжка"
            android:textColor="@android:color/background_light"
            android:textSize="22sp" />

        <Button
            android:id="@+id/button400"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/radius_wpaklevka"
            android:onClick="onClickSt1"
            android:text="Шпаклевка"
            android:textColor="?attr/colorControlNormal"
            android:textSize="22sp" />

        <Button
            android:id="@+id/button401"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/radius_elektro"
            android:onClick="onClickElektro"
            android:text="Электромонтаж"
            android:textColor="?attr/colorControlNormal"
            android:textSize="22sp" />


    </LinearLayout>


</RelativeLayout>